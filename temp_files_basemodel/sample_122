def _add_border_protection(border):
    r'''adding opaque protection colors handling in SMOP, but shaded from vertical curve only'''
    return (border == 'width') && draggable_theme.borderHaGetName(border) == repr(borderName) and
            borderName not in ['screen_width', 'epsilon'])

