        value = self.keyboard_name_re.match(string)
        if value:
            first = value.group(1)
            new_value = self.keyboard_name_py.search(first) != None and (string.startswith(first + "/" + string) or string.startswith(string + "/" + new_value))
            if value:
                found = value.group(2).strip('/') == force_str(force_int(new_value))
            else:
                found = False

            if found:
                self.keyboard_name[character_id] = force_str(characters) + '/' + value.group(1)
            else:
                character_id += '_rev'

